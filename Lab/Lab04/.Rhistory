randomMatrix <- function(p,q,values=-4:4){
matrix(sample(values,size=p*q,replace=TRUE),p,q)
}
set.seed(999)
A <- randomMatrix(3,2,values=-2:3)
B <- randomMatrix(2,2,values=-2:3)
C <- matrix(rep(0,4), rep(1,6), col = 3)
